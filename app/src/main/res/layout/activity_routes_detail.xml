<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/title1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32dp"
        android:layout_marginTop="14dp"
        android:text="HELLO"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        />

    <TextView
        android:id="@+id/location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title1"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="20dp"
        android:text="Start Location:"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/location_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title1"
        android:layout_marginStart="20dp"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/location"
        android:text="77"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/Steps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/location"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="20dp"
        android:text="Steps:"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/numOfSteps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/location"
        android:layout_marginStart="20dp"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/Steps"
        android:text="77"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/numOfMiles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/location"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="20dp"
        android:text="77888885"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/Miles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/location"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="20dp"
        android:layout_toStartOf="@+id/numOfMiles"
        android:text="Miles:"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/dur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Steps"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="20dp"
        android:text="Duration:"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/numOfDur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Steps"
        android:layout_marginStart="20dp"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/dur"
        android:text="77"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/numOfDay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="20dp"
        android:text="885"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/tags"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dur"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="20dp"
        android:text="Tags:"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tag1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dur"
        android:layout_marginStart="25dp"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/tags"
        android:text=""
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tag2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dur"
        android:layout_marginStart="15dp"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/tag1"
        android:text=""
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tag3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dur"
        android:layout_marginStart="15dp"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/tag2"
        android:text=""
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tag4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tags"
        android:layout_marginStart="25dp"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/tags"
        android:text=""
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tag5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tags"
        android:layout_marginStart="15dp"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/tag4"
        android:text=""
        android:textSize="20dp" />

    <Button
        android:id="@+id/start_preroute"
        android:layout_width="211sp"
        android:layout_height="75sp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        android:fontFamily="sans-serif-light"
        android:text="@string/start_walk_btn"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/Note"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tag4"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="20dp"
        android:text="Notes:"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/Note_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/Note"
        android:layout_above="@id/start_preroute"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="15dp"
        android:text="test"
        android:textSize="20dp" />


</RelativeLayout>